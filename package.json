    "react-dom": "18.3.1",
    "react-router-dom": "6.26.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.20",
    "postcss": "8.4.45",
    "prettier-plugin-tailwindcss": "0.6.6",
    "tailwindcss": "3.4.10",
    "vite": "5.4.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
